@model Character

<form asp-controller="Home" asp-action="RankingMasterResets">

    <h2 align="center">Top Master Resets</h2>
    <ul class="list-group">
        
            <li class="list-group-Model-success" align="center">  @Model.Name</li>
            <li class="list-group-Model" align="center"><img src="~/Characters/@Model.Image" class="img-rounded" height="200" width="200" /></li>
            <li class="list-group-Model" align="center"><small><strong>@Model.MrTotal</strong> Resets</small></li>
        
    </ul>
</form>